{
  "title": "businessForm",
  "type": "object",
  "required": [],
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      }
    },
    "businessType": {
      "type": "object",
      "properties": {
        "businessType": {
          "type": "string",
          "enum": ["Restaurant", "E-Commerce", "Home Improvement Contractor", "Other"]
        }
      }
    },
    "businessName": {
      "type": "object",
      "properties": {
        "businessName": {
          "type": "string"
        }
      }
    },
    "businessDescription": {
      "type": "object",
      "properties": {
        "businessDescription": {
          "type": "string"
        }
      }
    },
    "businessStructure": {
      "type": "object",
      "properties": {
        "businessStructure": {
          "type": "string",
          "enum": [
            "Sole Proprietorship",
            "General Partnership",
            "Limited Partnership (LP)",
            "Limited Liability Partnership (LLP)",
            "Limited Liability Company (LLC)",
            "C-Corporation",
            "S-Corporation",
            "B-Corporation"
          ]
        }
      }
    },
    "locations": {
      "type": "object",
      "properties": {
        "locations": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "properties": {
              "zipCode": {
                "type": "string"
              },
              "license": {
                "type": "boolean",
                "default": false
              }
            }
          }
        }
      }
    }
  }
}
